{
  "files.exclude": {
    "**/*.freezed.dart": true,
    "**/*.g.dart": true,
    "**/generated/*": true
  },
  "cSpell.words": [
    "AHSV",
    "altool",
    "argb",
    "Autovalidate",
    "Bitcode",
    "Celebi",
    "Checkmark",
    "chewie",
    "Cinemileage",
    "codesign",
    "comgooglemaps",
    "cupertino",
    "daddr",
    "deauthenticate",
    "Entame",
    "fukikae",
    "fullpath",
    "fullscreen",
    "gekijyo",
    "geolocator",
    "googleapis",
    "honpen",
    "ibsnet",
    "IMAX",
    "inappwebview",
    "JASS",
    "jikoku",
    "jimaku",
    "jyouei",
    "kanri",
    "kougyo",
    "kounyu",
    "kubun",
    "LTRB",
    "mobileprovision",
    "mylist",
    "numberpicker",
    "ogino",
    "Pausable",
    "prefcode",
    "qrcode",
    "quantizer",
    "recase",
    "Reclip",
    "Rgba",
    "riverpod",
    "roadmap",
    "saddr",
    "sakuhin",
    "shitei",
    "staticmap",
    "tcdx",
    "tearoffs",
    "Ticketless",
    "toho",
    "tohocinemas",
    "tsuboi",
    "tsuda",
    "unawaited",
    "unfocus",
    "vsync",
    "wzieba",
    "xcconfig",
    "xcrun",
    "xcscheme",
    "zipcloud",
    "zipcode",
    "zxing"
  ],
  "cSpell.ignorePaths": [
    "vscode-extension",
    "node_modules",
    ".git",
    ".vscode",
    "**/settings.json",
    ".firebaserc"
  ],
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true,
      "source.fixAll": true
    }
  },
  "dart.renameFilesWithClasses": "always",
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  "dart.flutterGenerateLocalizationsOnSave": "all",
  // Remove .fvm files from search
  "search.exclude": {
    "**/.fvm": true
  },
  // Remove from file watching
  "files.watcherExclude": {
    "**/.fvm": true
  }
}
